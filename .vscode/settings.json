{
  "search.followSymlinks": false,
  "explorer.openEditors.visible": 0,
  "workbench.startupEditor": "welcomePage",
  "window.titleBarStyle": "native",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.colorTheme": "GitHub Light",
  "workbench.sideBar.location": "right",
  "editor.formatOnSave": true,
  "editor.fontFamily": "'Cascadia Code', 'Fira Code','Monaco','Jetbrains mono','Source Han Sans CN'",
  "editor.fontLigatures": true,
  "editor.inlayHints.fontFamily": "'Cascadia Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 16,
  "editor.suggestFontSize": 16,
  "editor.roundedSelection": false,
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.lineHeight": 24,
  "editor.wordWrap": "on",
  "editor.detectIndentation": false,
  "editor.wordWrapColumn": 120,
  "editor.suggestSelection": "first",
  "editor.semanticHighlighting.enabled": false,
  "editor.suggest.preview": true,
  "editor.tabCompletion": "on",
  "editor.renderWhitespace": "all",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "phase",
  "editor.bracketPairColorization.enabled": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "never"
  },
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "*.wpy": "vue",
    "*.ttss": "css",
    "*.ttml": "html",
    "*.json": "jsonc"
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.syntaxProfiles": {
    "vue-html": "html",
    "vue": "html"
  },
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorBlinking": true,
  "debug.console.fontFamily": "'Cascadia Mono', 'Source Han Sans CN'",
  "git.confirmSync": false,
  "html.format.indentInnerHtml": true,
  "prettier.semi": true,
  "prettier.printWidth": 120,
  "prettier.singleQuote": true,
  "prettier.quoteProps": "as-needed",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.debug": true,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "css.lint.duplicateProperties": "error",
  "less.lint.duplicateProperties": "error",
  "scss.lint.duplicateProperties": "error",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "javascript.inlayHints.enumMemberValues.enabled": false,
  "javascript.inlayHints.parameterTypes.enabled": false,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": false,
  "javascript.inlayHints.variableTypes.enabled": false,
  "typescript.inlayHints.enumMemberValues.enabled": false,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": false,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": false,
  "typescript.inlayHints.variableTypes.enabled": false,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "github.gitAuthentication": false,
  "cSpell.diagnosticLevel": "Error",
  "npm.packageManager": "pnpm",
  "files.eol": "\n",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "yaml.maxItemsComputed": 50000,
  "redhat.telemetry.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.enablePreview": false,
  "svg.preview.mode": "svg",
  "resmon.show.cpufreq": false,
  "resmon.updatefrequencyms": 1000
}
